generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user_details {
  user_id    Int    @id @default(autoincrement())
  username   String
  first_name String
  last_name  String
  gender     String
  password   String
  status     Int
}
